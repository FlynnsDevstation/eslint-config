{
  "name": "@flynns-devstation/eslint-config",
  "version": "1.1.0",
  "description": "ESlint config for flynns devstation",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --config=eslintrc.json",
    "format": "prettier --write '**/*.{js,md,ts}'"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "react",
    "typescript"
  ],
  "author": {
    "name": "Chris Helgert",
    "email": "chris@flynnsdevstation.de"
  },
  "bugs": {
    "url": "https://github.com/flynnsDevstation/eslint-config"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/flynnsDevstation/eslint-config"
  },
  "license": "MIT",
  "devDependencies": {
    "@flynns-devstation/prettier-config": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.3.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*{js,json,md,ts}": [
      "prettier --write"
    ]
  }
}
